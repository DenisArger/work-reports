{
  "name": "worker-reports-bot",
  "private": true,
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "dev": "tsc -p tsconfig.json && tsx scripts/devServer.mjs",
    "build": "tsc -p tsconfig.json",
    "build:vercel": "node scripts/build-vercel-output.mjs",
    "webhook:local": "tsx scripts/setWebhook.mjs local",
    "webhook:prod": "tsx scripts/setWebhook.mjs prod",
    "webhook:delete": "tsx scripts/deleteWebhook.mjs",
    "webhook:info": "tsx scripts/webhookInfo.mjs",
    "lint": "tsc -p tsconfig.json --noEmit",
    "test": "node --test test/**/*.test.*"
  },
  "dependencies": {
    "googleapis": "^156.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.13.0",
    "@vercel/node": "^5.5.28",
    "dotenv": "^17.2.3",
    "esbuild": "^0.27.2",
    "tsx": "^4.21.0",
    "typescript": "^5.7.3",
    "vercel": "^50.9.5"
  }
}
